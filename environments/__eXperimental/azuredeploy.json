{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "ClientId": {
            "reference": {
                "keyVault": {
                    "id": "$(LabKeyVaultId)"
                },
                "secretName": "ClientId"
            }
        },
        "ClientSecret": {
            "reference": {
                "keyVault": {
                    "id": "$(LabKeyVaultId)"
                },
                "secretName": "ClientSecret"
            }
        }
    },
    "variables": {},
    "resources": [],
    "outputs": {
        "KeyVaultId": {
            "type": "string",
            "value": "$(LabKeyVaultId)"
        },
        "ClientId": {
            "type": "string",
            "value": "[parameters('ClientId')]"
        },
        "ClientSecret": {
            "type": "string",
            "value": "[parameters('ClientSecret')]"
        }
    }
}