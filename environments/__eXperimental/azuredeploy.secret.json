{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "KeyVaultId" :{
            "type": "string"
        },
        "ClientId": {
            "reference": {
                "keyVault": {
                    "id": "[parameters('KeyVaultId')]"
                },
                "secretName": "ClientId"
            }
        }
    },
    "resources": [],
    "outputs": {
        "ClientId": {
            "type": "string",
            "value": "[parameters('ClientId')]"
        }
    }
}